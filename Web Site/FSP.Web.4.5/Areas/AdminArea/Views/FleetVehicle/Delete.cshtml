@model FSP.Domain.Model.FleetVehicle

@{
    ViewBag.Title = "Delete " + @Model.VehicleNumber;
    
}

<h2>Delete "@Model.VehicleNumber"</h2>

<h3>Are you sure you want to retire this fleet vehicle?</h3>

@using (Html.BeginForm())
{
    <p>
        <div class="form-actions">
            <button type="submit" class="btn btn-danger">
                Retire Fleet Vehicle</button>
        </div>
    </p>
}
