@model FSP.Domain.Model.YearlyCalendar

@{
    ViewBag.Title = "Delete " + @Model.dayName;
    
}

<h2>Delete "@Model.dayName"</h2>

<h3>Are you sure you want to delete this yearly calendary day?</h3>

@using (Html.BeginForm())
{
   
        <div class="form-actions">
            <button type="submit" class="btn btn-danger">
                Delete</button>
        </div>
   
}
