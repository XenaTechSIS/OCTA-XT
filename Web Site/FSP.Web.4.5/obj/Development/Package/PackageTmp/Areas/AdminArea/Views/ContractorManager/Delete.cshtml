@model FSP.Domain.Model.ContractorManager

@{
    ViewBag.Title = "Delete " + @Model.FirstName + " " + @Model.LastName;
    
}

<h2>Delete "@Model.FirstName @Model.LastName"</h2>

<h3>Are you sure you want to delete this contractor manager?</h3>

@using (Html.BeginForm())
{
    <p>
        <div class="form-actions">
            <button type="submit" class="btn btn-danger">
                Delete</button>
        </div>
    </p>
}
