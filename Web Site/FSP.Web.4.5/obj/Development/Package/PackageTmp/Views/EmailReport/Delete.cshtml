@model FSP.Domain.Model.AERecipient

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this email report recipient?</h3>
<fieldset>
    <legend>"@Model.AERecipientEmail"</legend>
</fieldset>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a href="~/EmailReport/Index" class="btn">Cancel</a>
    </p>
}
