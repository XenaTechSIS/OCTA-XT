@model FSP.Domain.Model.ServiceType
@{
    ViewBag.Title = "Edit " + @Model.ServiceType1;
    
}
<div>
    @Html.ActionLink("Back to Tow Location", "Index")
</div>
<h2>Edit "@Model.ServiceType1"</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <fieldset>
        <legend></legend>
        @Html.HiddenFor(model => model.ServiceTypeID)
        <div class="editor-label">
            @Html.LabelFor(model => model.ServiceType1)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ServiceType1)
            @Html.ValidationMessageFor(model => model.ServiceType1)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.ServiceTypeCode)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ServiceTypeCode)
            @Html.ValidationMessageFor(model => model.ServiceTypeCode)
        </div>
        <div class="form-actions">
            <button type="submit" class="btn btn-primary">
                Update</button>
        </div>
    </fieldset>
}
