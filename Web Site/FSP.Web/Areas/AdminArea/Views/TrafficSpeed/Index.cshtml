@model IEnumerable<FSP.Domain.Model.TrafficSpeed>
@{
    ViewBag.Title = "Traffic Speed";
}

<p>
    @Html.ActionLink("Add Traffic Speed", "Create")
</p>
<h2>Traffic Speed</h2>
<table class="table table-bordered table-condensed table-striped sortable">
    <thead>
        <tr>
            <th>Name
            </th>
            <th>Code
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(item.TrafficSpeed1, "Edit", new { id = item.TrafficSpeedID })
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TrafficSpeedCode)
                </td>
                <td>
                    @Html.ActionLink("Delete", "Delete", new { id = item.TrafficSpeedID })
                </td>
            </tr>
        }
    </tbody>
</table>
