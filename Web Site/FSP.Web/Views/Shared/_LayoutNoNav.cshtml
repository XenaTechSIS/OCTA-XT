@using FSP.Web.Helpers
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    @*This meta tag will make IE browsers feel like Chrome*@
    @*  <meta http-equiv="X-UA-Compatible" content="chrome=1">*@
    <link href="@Url.Content("~/OCTAFav.ico")" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title</title>    
    <link href="@Url.Content("~/Content/jquery.layout.latest.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/themes/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/bootstrap/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    @*<link href="@Url.Content("~/Content/bootstrap.2.0.1.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap-responsive.2.0.1.css")" rel="stylesheet" />*@

    <link href="@Url.Content("~/Content/docs.css")" rel="stylesheet" type="text/css" />
